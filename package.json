{
  "name": "@firstandthird/jsonp",
  "version": "1.0.1",
  "description": "Quick JSONP creator",
  "main": "index.js",
  "module": "dist/jsonp.esm.js",
  "scripts": {
    "start": "scriptkit",
    "build": "scriptkit build",
    "dev": "scriptkit dev",
    "pretest": "npm run build",
    "test": "cat test/jsonp.test.dist.js | tape-run --static . --browser phantomjs --render tap-spec",
    "prepublishOnly": "scriptkit"
  },
  "repository": "https://github.com/firstandthird/jsonp",
  "author": "First+Third",
  "license": "MIT",
  "devDependencies": {
    "eslint-config-firstandthird": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "phantomjs-prebuilt": "^2.1.16",
    "scriptkit": "^2.0.3",
    "tap-spec": "^5.0.0",
    "tape-rollup": "^4.6.4",
    "tape-run": "^5.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "firstandthird"
  },
  "scriptkit": {
    "files": {
      "dist/jsonp.js": "index.js",
      "test/jsonp.test.dist.js": "test/jsonp.test.js"
    },
    "scripts": {
      "commonjs": {
        "ignore": [
          "node_modules/tape-rollup/*"
        ]
      }
    }
  }
}
